{
    "name": "laravel-tabulator-vue",
    "packageManager": "yarn@3.2.1",
    "version": "0.0.2",
    "main": "dist/tabulator-table.js",
    "module": "dist/tabulator-table.esm.js",
    "umd": "dist/tabulator-table.esm.js",
    "scripts": {
        "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
        "build:umd": "rollup --config ./rollup.config.js --format umd --file dist/tabulator-table.umd.js",
        "build:es": "rollup --config ./rollup.config.js --format es --file dist/tabulator-table.esm.js",
        "build:unpkg": "rollup --config ./rollup.config.js --format iife --file dist/tabulator-table.min.js"
    },
    "dependencies": {
        "@vue/composition-api": "^1.4.6",
        "jquery": "^3.6.0",
        "postcss": "^8.4.14",
        "vue": "^2.6.14"
    },
    "devDependencies": {
        "@babel/parser": "^7.18.6",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@types/node": "^16.11.25",
        "@vue/runtime-dom": "^3.2.31",
        "lodash": "^4.17.21",
        "redom": "^3.29.0",
        "rollup": "^1.17.0",
        "rollup-plugin-vue": "^5.0.1",
        "tabulator-tables": "^5.2.7",
        "uuid": "^8.3.2",
        "vue-template-babel-compiler": "^1.2.0",
        "vue-template-compiler": "^2.6.14"
    }
}
